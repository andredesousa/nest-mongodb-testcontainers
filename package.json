{
  "name": "nest-mongodb-testcontainers",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "nest start --watch",
    "start:debug": "nest start --watch --debug",
    "start:prod": "npm run build && node dist/main.js",
    "check": "npm run lint && npm run test",
    "lint": "eslint \"{src,e2e}/**/*.ts\"",
    "lint:fix": "eslint \"{src,e2e}/**/*.ts\" --fix",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "clean": "rimraf dist/ reports/",
    "e2e": "jest --config e2e/jest.config.js",
    "e2e:watch": "jest -c e2e/jest.config.js --watch",
    "e2e:debug": "node --inspect-brk node_modules/jest/bin/jest.js -c e2e/jest.config.js --runInBand",
    "prebuild": "rimraf dist",
    "build": "nest build"
  },
  "dependencies": {
    "@nestjs/common": "8.4.3",
    "@nestjs/core": "8.4.3",
    "@nestjs/mongoose": "9.0.3",
    "@nestjs/platform-express": "8.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "mongoose": "6.2.10",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.4",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/testing": "8.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "jest-html-reporters": "^3.0.6",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.3.0",
    "testcontainers": "^8.6.1",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-mockery": "^1.2.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
